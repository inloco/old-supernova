{
  "name": "supernova-inloco",
  "version": "4.2.8",
  "description": "In Loco Media Front-End Packages",
  "main": "./lib/index.js",
  "author": "In Loco Media Front-End Team",
  "license": "ISC",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/ubeedev/supernova.git"
  },
  "scripts": {
    "build:sass": "node-sass assets/styles/basic.scss assets/css/basic.css",
    "build:js": "babel src -d lib -q",
    "build": "npm run build:js && npm run build:sass",
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "start-dev:doc": "cd docs && node index.js",
    "postinstall": "npm run build",
    "coverage": "node scripts/coverage.js"
  },
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "material-design-icons": "^3.0.1"
  },
  "jest": {
    "testRegex": "(.*)(-|.)test.js$",
    "testPathIgnorePatterns": [
      "<rootDir>/lib/",
      "<rootDir>/build/"
    ],
    "setupFiles": [
      "<rootDir>/shim.js",
      "<rootDir>/setupTests.js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx"
    ],
    "moduleNameMapper": {
      "\\.(css|jpg|png|svg)$": "<rootDir>/tests/utils/empty-module.js"
    },
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "classnames": "^2.2.5",
    "material-design-icons": "^3.0.1",
    "moment": "^2.20.1",
    "node-sass": "^4.8.3",
    "prop-types": "^15.6.0",
    "react": "^16.5.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.2.0",
    "react-highlight": "github:briancappello/react-highlight#react-v16-compiled",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2"
  },
  "devDependencies": {
    "babel-jest": "^21.2.0",
    "chalk": "^2.3.0",
    "child_process": "^1.0.2",
    "dotenv": "^4.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "jest": "^21.2.1",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.2.0"
  }
}
