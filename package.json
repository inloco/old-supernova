{
  "name": "supernova-inloco",
  "version": "2.1.0",
  "description": "In Loco Media Front-End Packages",
  "main": "./build/index.js",
  "author": "In Loco Media Front-End Team",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/ubeedev/supernova.git"
  },
  "scripts": {
    "build": "rm -rf build && babel src -d build",
    "build-dev": "rm -rf build && webpack --config webpack.dev.config.js --watch",
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "start-dev:doc": "cd docs && node index.js",
    "eslint": "eslint ./src --ext .js --ext .jsx --cache || true",
    "eslint:table": "eslint ./src --ext .js --ext .jsx --cache --format table || true",
    "eslint:summary": "eslint ./src --ext .js --ext .jsx --cache --format ‘node_modules/eslint-friendly-formatter’ || true",
    "postinstall": "npm run build"
  },
  "peerDependencies": {
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "material-design-icons": "^2.2.3"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.7",
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.9.0",
    "css-loader": "^0.23.1",
    "enzyme": "^2.5.1",
    "eslint": "^3.10.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-friendly-formatter": "^2.0.6",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.7.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "jest": "^17.0.0",
    "node-sass": "^3.8.0",
    "react": "^15.4.1",
    "react-addons-test-utils": "^15.3.2",
    "react-dom": "^15.4.1",
    "sass-loader": "^4.0.0",
    "sinon": "^1.17.6",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14"
  },
  "jest": {
    "testRegex": "(.*)-test.js$",
    "testPathIgnorePatterns": [
      "<rootDir>/build/"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx"
    ]
  }
}
