{
  "name": "supernova-inloco",
  "version": "0.1.8",
  "description": "In Loco Media Front-End Packages",
  "main": "./build/index.js",
  "author": "In Loco Media Front-End Team",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/ubeedev/supernova.git"
  },
  "scripts": {
    "build": "rm -rf build && webpack -p",
    "build-dev": "rm -rf build && webpack --config webpack.dev.config.js --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "start-dev:doc": "cd docs && node index.js"
  },
  "peerDependencies": {
    "react": "^15.3.0",
    "react-dom": "^15.3.0"
  },
  "dependencies": {
    "material-design-icons": "^2.2.3"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.7",
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "css-loader": "^0.23.1",
    "enzyme": "^2.5.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "jest": "^17.0.0",
    "node-sass": "^3.8.0",
    "react-addons-test-utils": "^15.3.2",
    "react-dom": "^15.3.0",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "react": "^15.3.0"
  },
  "jest": {
    "testRegex": "(.*)-test.js$",
    "testPathIgnorePatterns": [
      "<rootDir>/build/"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx"
    ]
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ]
  }
}
