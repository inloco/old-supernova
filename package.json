{
  "name": "supernova-inloco",
  "version": "2.1.0",
  "description": "In Loco Media Front-End Packages",
  "main": "./lib/index.js",
  "author": "In Loco Media Front-End Team",
  "license": "ISC",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/ubeedev/supernova.git"
  },
  "scripts": {
    "build": "babel src -d lib -q",
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "start-dev:doc": "cd docs && node index.js",
    "postinstall": "npm run build",
    "coverage": "node scripts/coverage.js"
  },
  "peerDependencies": {
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "material-design-icons": "^2.2.3"
  },
  "jest": {
    "testRegex": "(.*)-test.js$",
    "testPathIgnorePatterns": [
      "<rootDir>/lib/",
      "<rootDir>/build/"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx"
    ],
    "moduleNameMapper": {
      "\\.(css|jpg|png|svg)$": "<rootDir>/tests/utils/empty-module.js"
    },
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  },
  "dependencies": {
    "prop-types": "^15.5.9",
    "react": "^15.5.4",
    "react-copy-to-clipboard": "^4.2.3",
    "react-dom": "^15.5.4",
    "react-highlight": "^0.10.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-jest": "^20.0.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chalk": "^1.1.3",
    "child_process": "^1.0.2",
    "dotenv": "^4.0.0",
    "enzyme": "^2.8.2",
    "jest": "^17.0.3",
    "react-addons-test-utils": "^15.5.1",
    "react-test-renderer": "^15.5.4"
  }
}
