{
  "name": "supernova-inloco",
  "version": "0.1.8",
  "description": "In Loco Media Front-End Packages",
  "main": "./build/index.js",
  "author": "In Loco Media Front-End Team",
  "license": "ISC",
  "scripts": {
    "build": "babel ./src --out-dir ./build",
    "build:watch": "babel ./src --out-dir ./build --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "postinstall": "npm run build",
    "eslint": "eslint ./src --ext .js --ext .jsx --cache || true",
    "eslint:table": "eslint ./src --ext .js --ext .jsx --cache --format table || true",
    "eslint:summary": "eslint ./src --ext .js --ext .jsx --cache --format 'node_modules/eslint-friendly-formatter' || true"
  },
  "dependencies": {
    "enzyme": "^2.6.0",
    "prop-types": "^15.5.10",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-google-charts": "^1.0.0",
    "sinon": "^1.17.6"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.5",
    "babel-cli": "^6.10.1",
    "babel-core": "^6.7.4",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.6.1",
    "css-loader": "^0.23.1",
    "es5-shim": "^4.5.7",
    "eslint": "^3.10.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-friendly-formatter": "^2.0.6",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.7.1",
    "expose-loader": "^0.7.1",
    "imports-loader": "^0.6.5",
    "jest-cli": "^17.0.3",
    "loader-utils": "^0.2.13",
    "react-addons-test-utils": "~15.1.0",
    "webpack": "^1.12.14"
  },
  "jest": {
    "testRegex": "(.*)-test.js$",
    "testPathIgnorePatterns": [
      "<rootDir>/build/"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react/",
      "<rootDir>/node_modules/react-dom/",
      "<rootDir>/node_modules/react-addons-test-utils/",
      "<rootDir>/src/helpers/"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx"
    ]
  }
}
